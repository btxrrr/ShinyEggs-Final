
<h2>Audit Details</h2>

<table>
    <thead>
        <tr>
            <th>Date</th>
            <th>Message</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
                <tr>
                    <td>@log.Timestamp</td>
                    <td>@log.Message</td>
                </tr>
        }
    </tbody>
</table>